{
	"compilerOptions": {
		"declaration": true,
		"esModuleInterop": true,
		"exactOptionalPropertyTypes": true,
		"isolatedModules": true,
		"module": "ES2020",
		"moduleResolution": "node",
		"noImplicitOverride": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"outDir": "lib",
		"sourceMap": true,
		"strict": true,
		"target": "ES2020"
	}
}